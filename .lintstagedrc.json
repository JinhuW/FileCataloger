{
  "src/**/*.{ts,tsx}": [
    "prettier --write",
    "eslint --fix",
    "bash -c 'yarn typecheck'"
  ],
  "src/**/*.{js,jsx}": [
    "prettier --write",
    "eslint --fix"
  ],
  "src/**/*.{json,css,scss,md}": [
    "prettier --write"
  ],
  "package.json": [
    "prettier --write"
  ],
  "*.md": [
    "prettier --write"
  ]
}